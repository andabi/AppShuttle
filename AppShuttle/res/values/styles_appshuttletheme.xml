<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewAppShuttleTheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/appshuttletheme_list_selector_holo_light</item>
	    <item name="android:background">@drawable/appshuttletheme_edit_text_holo_light</item>
  </style>

  <style name="ButtonAppShuttleTheme" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/appshuttletheme_btn_default_holo_light</item>
  </style>

  <style name="ImageButtonAppShuttleTheme" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/appshuttletheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerAppShuttleTheme" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/appshuttletheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/appshuttletheme_list_selector_holo_light</item>
  </style>

  <style name="TabAppShuttleTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/appshuttletheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextAppShuttleTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ToggleAppShuttleTheme" parent="android:Widget.Holo.Light.Button.Toggle">
      <item name="android:background">@drawable/appshuttletheme_btn_toggle_holo_light</item>
  </style>

</resources>